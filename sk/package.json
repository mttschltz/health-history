{
  "name": "sk",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "dev:backend": "cd ../pb && modd",
    "backend": "AUDITLOG=users,posts ../pb/entrypoint.sh ./pocketbase serve --publicDir=../sk/build",
    "build": "vite build",
    "build:backend": "cd ../pb && go build",
    "typegen": "pocketbase-typegen --db ../pb/pb_data/data.db --out ./src/lib/pocketbase/generated-types.ts",
    "test": "playwright test",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@skeletonlabs/skeleton-svelte": "^1.0.0-next.4",
    "@sveltejs/adapter-auto": "^3.0.0",
    "@sveltejs/adapter-static": "^3.0.1",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/node": "^20.14.11",
    "autoprefixer": "^10.4.19",
    "fast-deep-equal": "^3.1.3",
    "pocketbase-typegen": "^1.2.1",
    "postcss": "^8.4.38",
    "prettier": "^3.1.1",
    "prettier-plugin-svelte": "^3.1.2",
    "prettier-plugin-tailwindcss": "^0.6.4",
    "sass": "^1.77.1",
    "svelte": "^5.0.0-next.1",
    "svelte-check": "^3.6.0",
    "svelte-preprocess": "^5.1.4",
    "tailwindcss": "^3.4.4",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3"
  },
  "type": "module",
  "dependencies": {
    "@skeletonlabs/skeleton": "^3.0.0-next.2",
    "@skeletonlabs/tw-plugin": "^0.4.0",
    "lucide-svelte": "^0.411.0",
    "pocketbase": "^0.21.2",
    "zod": "^3.23.8"
  }
}
